@model PruebaGym2.Models.SocioRutina

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>SocioRutina</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Socio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Socio.Mail)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rutina)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rutina.Descripcion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Calificacion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Calificacion)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { idSocio = Model.IdSocio, idRutina = Model.IdRutina }) |
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</div>
